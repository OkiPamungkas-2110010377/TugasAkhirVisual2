unit TblSemester;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, Grids, DBGrids, DB, ZAbstractRODataset, ZAbstractDataset,
  ZDataset, ZAbstractConnection, ZConnection, StdCtrls, ComCtrls;

type
  TForm9 = class(TForm)
    l1: TLabel;
    l2: TLabel;
    l3: TLabel;
    l4: TLabel;
    l5: TLabel;
    l8: TLabel;
    l9: TLabel;
    l7: TLabel;
    Edit1: TEdit;
    Edit2: TEdit;
    c1: TComboBox;
    c2: TComboBox;
    c3: TComboBox;
    c4: TComboBox;
    c5: TComboBox;
    dtp1: TDateTimePicker;
    b1: TButton;
    b2: TButton;
    b3: TButton;
    b4: TButton;
    b5: TButton;
    b6: TButton;
    con1: TZConnection;
    zqry1: TZQuery;
    zqry2: TZQuery;
    zqry3: TZQuery;
    zqry4: TZQuery;
    zqry5: TZQuery;
    zqry6: TZQuery;
    ds1: TDataSource;
    dg1: TDBGrid;
    procedure posisiawal;
    procedure editenable;
    procedure editbersih;
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form9: TForm9;

implementation

{$R *.dfm}

procedure TForm9.posisiawal;
begin
editbersih;

c1.Enabled:= false;
c2.Enabled:= false;
c3.Enabled:= false;
c4.Enabled:= false;
c5.Enabled:= false;
dtp1.Enabled:= False;
edit1.Enabled:= false;
edit2.Enabled:= false;


b1.Enabled:= True;
b2.Enabled:= False;
b3.Enabled:= False;
b4.Enabled:= False;
b5.Enabled:= False;
end;

procedure TForm9.editenable;
begin
c1.Enabled:= True;
c2.Enabled:= True;
c3.Enabled:= True;
c4.Enabled:= True;
c5.Enabled:= True;
dtp1.Enabled:=True;
edit1.Enabled:= True;
edit2.Enabled:= True;

end;

procedure TForm9.editbersih;
begin
c1.text:= '';
c2.text:= '';
c3.text:= '';
c4.text:= '';
c5.text:= '';
Edit1.Clear;
Edit2.Clear;

end;

end.
